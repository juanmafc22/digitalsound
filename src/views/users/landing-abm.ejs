<!DOCTYPE html>
<html lang="es">
<head>
    <%- include("../partials/head") %>
</head>
<body>

    <%- include("../partials/header-modulo-admin") %>

    <main>
        <!-- Seccion del main para listar las diferentes categorias
            1. Guitarras y bajos
            2. Baterias
            3. Amplificadores
            4. Accesorios
        -->

        <div class="div-titulo-boton-comprar">
            <h2 class="titulo-abm-prods">ABM de USUARIOS: Alta, Baja o Modificaci√≥n.</h2>
        </div>

        <section class="prods-destacados">

            <article class="articulo-detalle-desktop">
            
                <form action="alta-usuario">
                    <div>
                        <button class="boton-abm" type="submit">Dar de ALTA un usuario nuevo</button>
                    </div>
                </form>

                <p class="texto-abm">Elegir un usuario para Modificar o Dar de Baja</p>
                <p>
                    <% usuarios.forEach (usuario => { %> 
                    <a href="/users/editar/<%= usuario.id %>">Editar</a> | <a href="/users/baja/<%= usuario.id %>">Eliminar</a> | ID <%= usuario.id %> | <%= usuario.nombre %> <%= usuario.apellido %> <br>
                    <% }) %> 
                </p>
                <br>
                 
                
            </article>

          
        </section>
        
    </main>

    <%- include("../partials/footer") %>
    <!-- 
    <script src="/js/alert-landing-abm.js"></script>
    -->
    
</body>
</html>